{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "vite build",
    "beforeDevCommand": "vite",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "剑网3改键工具",
        "width": 960,
        "height": 510,
        "minWidth": 860,
        "minHeight": 570,
        "center": true,
        "decorations": false,
        "titleBarStyle": "Overlay"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ]
  },
  "identifier": "com.jx3.keymanager",
  "productName": "剑网3改键工具",
  "version": "3.0.0"
}